package Main;

public class User {
	private int uid;
	private String uname;
	private String umail;
	
	public User() {
		this.uid = 0;
		this.uname = "";
		this.umail = "";
	}
	public int getUid() {
		return this.uid;
	}
	public void setUid(int uid) {
		this.uid = uid;
	}
	public String getUname() {
		return uname;
	}
	public void setUname(String uname) {
		this.uname = uname;
	}
	public String getUmail() {
		return umail;
	}
	public void setUmail(String umail) {
		this.umail = umail;
	}
	
}
