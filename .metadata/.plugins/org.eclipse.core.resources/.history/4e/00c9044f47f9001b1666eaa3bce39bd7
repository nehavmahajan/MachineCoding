package models;

import java.util.ArrayList;

import repository.Gender;

public class User {
	private  String name;
	private Gender gender;
	private String phoneNumber;
	private ArrayList<String> pincode;

	public User() {
		
	}
	public User(String name, Gender gender, String phoneNumber, ArrayList<String> pincode) {
		this.name = name;
		this.gender = gender;
		this.phoneNumber = phoneNumber;
		this.pincode = pincode;
	}
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Gender getGender() {
		return gender;
	}

	public void setGender(Gender gender) {
		this.gender = gender;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	public ArrayList<String> getPincode() {
		return pincode;
	}

	public void setPincode(ArrayList<String> pincode) {
		this.pincode = pincode;
	}
}
