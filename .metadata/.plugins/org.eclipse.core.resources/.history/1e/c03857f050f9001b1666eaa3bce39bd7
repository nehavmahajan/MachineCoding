package repository;

import java.util.Comparator;

import models.Restaurant;

public class sortRestaurantByRating implements Comparator {

	public sortRestaurantByRating() {}
	@Override
	public int compare(Object o1, Object o2) {
		if(((Restaurant) o1).getRating().getRatingnumber() == ((Restaurant) o2).getRating().getRatingnumber()) {
			return 0;
		}
		if(((Restaurant) o1).getRating().getRatingnumber() > ((Restaurant) o2).getRating().getRatingnumber()) {
			return 1;
		}
		return -1;
	}
//	@Override
//	public int compare(Object o1, Object o2) {
//		// TODO Auto-generated method stub
//		return 0;
//	}

}
